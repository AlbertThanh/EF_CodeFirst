@model List<EF_CodeFirst.Models.Category>
@{
    ViewBag.Title = "Index";
}

<h2>Categories</h2>
<table class="table">
    <tr>
        <th>Category ID</th>
        <th>Category Name</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CategoryID</td>
            <td>@item.CategoryName</td>
        </tr>
    }
</table>

